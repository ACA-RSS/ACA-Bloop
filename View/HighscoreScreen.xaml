<Window x:Class="Twisted_Treeline.View.HighscoreScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Twisted_Treeline.View"
        mc:Ignorable="d"
        Title="HighscoreScreen" Height="300" Width="300" Background="DarkSlateGray">
    <Grid>
        <TextBlock x:Name="Title" Text="HALL OF FAME" Foreground="Goldenrod" HorizontalAlignment="Center"  VerticalAlignment="Top" FontSize="20"/>
        <TextBlock x:Name="Scores" HorizontalAlignment="Center" Margin="0,25,0,0" Width="250" Background="DarkSlateGray" FontSize="20" Foreground="Goldenrod"/>

    </Grid>
</Window>
