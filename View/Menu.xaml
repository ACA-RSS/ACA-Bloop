<Window x:Class="Twisted_Treeline.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Twisted_Treeline"
        mc:Ignorable="d"
        Title="Menu" Height="300" Width="300" Background="LightGreen">
    <Grid>
        <Button x:Name="btnSave" Content="Save" HorizontalAlignment="Center" Margin="0,68,0,0" VerticalAlignment="Top" Width="75" Click="btnSave_Click"/>
        <Button x:Name="btnAbout" Content="About" HorizontalAlignment="Center" Margin="0,123,0,0" VerticalAlignment="Top" Width="75" Click="btnAbout_Click"/>
        <Button x:Name="btnHelp" Content="Help" HorizontalAlignment="Center" Margin="0,180,0,0" VerticalAlignment="Top" Width="75" Click="btnHelp_Click"/>


    </Grid>
</Window>
