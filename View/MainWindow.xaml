<Window x:Class="Twisted_Treeline.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Twisted_Treeline"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="1440" Background="Black">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="273*"/>
            <ColumnDefinition Width="1160*"/>
        </Grid.ColumnDefinitions>
        <Image HorizontalAlignment="Left" Height="118" VerticalAlignment="Top" Width="817" Source="TTLine.png" Grid.Column="1" Margin="29.2,0,0,0"/>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Difficulty Level:" FontSize ="15" Foreground="White" VerticalAlignment="Top" RenderTransformOrigin="0.68,0.541" Grid.Column="1" Margin="682.2,180,0,0" Height="22" Width="120"/>
        <ComboBox Text="HERE" Foreground="White" Margin="807.2,180,80.6,668.4" Grid.Column="1" >
            <ComboBoxItem Name="cbi1">Item1</ComboBoxItem>
            <ComboBoxItem Name="cbi2">Item2</ComboBoxItem>
            <ComboBoxItem Name="cbi3">Item3</ComboBoxItem>
        </ComboBox>
        <Button Content="Start Game" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Grid.Column="1" Margin="420.2,165,0,0" Click="Button_Click"/>
        <Button Content="About" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Grid.Column="1" Margin="420.2,211,0,0"/>
        <Button Content="Help" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Grid.Column="1" Margin="420.2,257,0,0"/>
        <Button Content="High Scores" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Grid.Column="1" Margin="420.2,306,0,0"/>
        


    </Grid>
</Window>
